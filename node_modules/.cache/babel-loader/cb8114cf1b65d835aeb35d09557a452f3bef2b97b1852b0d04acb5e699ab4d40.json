{"ast":null,"code":"import Message from \"./Message.vue\";\nexport default {\n  name: \"Dashboard\",\n  data() {\n    return {\n      burgers: null,\n      burger_id: null,\n      status: [],\n      msg: null,\n      data: {\n        paes: [],\n        // Coloque aqui os tipos de pães\n        carnes: [],\n        // Coloque aqui os tipos de carnes\n        opcionais: [] // Coloque aqui os tipos de opcionais\n      }\n    };\n  },\n\n  methods: {\n    async getPedidos() {\n      const req = await fetch(\"http://localhost:3000/burgers\");\n      const data = await req.json();\n      this.burgers = data;\n      this.getStatus();\n    },\n    async getStatus() {\n      const req = await fetch(\"http://localhost:3000/status\");\n      const data = await req.json();\n      this.status = data;\n    },\n    async deleteBurger(id) {\n      const req = await fetch(`http://localhost:3000/burgers/${id}`, {\n        method: \"DELETE\"\n      });\n      const res = await req.json();\n      this.msg = `Pedido Removido com Sucesso!`;\n      setTimeout(() => {\n        this.msg = \"\";\n      }, 5000);\n      this.getPedidos();\n    },\n    async updateBurger(event, id) {\n      const option = event.target.value;\n      const dataJson = JSON.stringify({\n        status: option\n      });\n      const req = await fetch(`http://localhost:3000/burgers/${id}`, {\n        method: \"PATCH\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: dataJson\n      });\n      const res = await req.json();\n      this.msg = `O Pedido Nº ${res.id} foi atualizado para Status ${res.status}!`;\n      setTimeout(() => {\n        this.msg = \"\";\n      }, 5000);\n    },\n    async getPaes() {\n      const response = await fetch(\"http://localhost:3000/paes\");\n      const data = await response.json();\n      this.data.paes = data;\n    },\n    async getCarnes() {\n      const response = await fetch(\"http://localhost:3000/carnes\");\n      const data = await response.json();\n      this.data.carnes = data;\n    },\n    async getOpcionais() {\n      const response = await fetch(\"http://localhost:3000/opcionais\");\n      const data = await response.json();\n      this.data.opcionais = data;\n    }\n  },\n  async mounted() {\n    await this.getPedidos();\n    await this.getPaes();\n    await this.getCarnes();\n    await this.getOpcionais();\n  },\n  components: {\n    Message\n  }\n};","map":{"version":3,"names":["Message","name","data","burgers","burger_id","status","msg","paes","carnes","opcionais","methods","getPedidos","req","fetch","json","getStatus","deleteBurger","id","method","res","setTimeout","updateBurger","event","option","target","value","dataJson","JSON","stringify","headers","body","getPaes","response","getCarnes","getOpcionais","mounted","components"],"sources":["D:\\Vuejs\\3_make_your_burger\\src\\components\\Dashboard.vue"],"sourcesContent":["<template>\r\n    <div id=\"burger-table\" v-if=\"burgers && burgers.length > 0\">\r\n      <div>\r\n        <Message :msg=\"msg\" v-show=\"msg\" />\r\n        <div id=\"burger-table-heading\">\r\n          <div class=\"order-id\">#</div>\r\n          <div>Clientes:</div>\r\n          <div>Pão:</div>\r\n          <div>Carne:</div>\r\n          <div>Opcionais:</div>\r\n          <div>Ações</div>\r\n        </div>\r\n      </div>\r\n      <div id=\"burger-table-rows\">\r\n        <div class=\"burger-table-row\" v-for=\"burger in burgers\" :key=\"burger.id\">\r\n          <div class=\"order-number\">{{ burger.id }}</div>\r\n          <div>{{ burger.nome }}</div>\r\n          <div>{{ getPaoType(burger.pao) }}</div>\r\n          <div>{{ getCarneType(burger.carne) }}</div>\r\n          <div>\r\n            <ul>\r\n              <li v-for=\"(opcional, index) in burger.opcionais\" :key=\"index\">\r\n                {{ getOpcionalType(opcional) }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <div>\r\n            <select\r\n              name=\"status\"\r\n              class=\"status\"\r\n              @change=\"updateBurger($event, burger.id)\"\r\n            >\r\n              <option value=\"\">Selecione</option>\r\n              <option v-for=\"s in status\" :key=\"s.id\" :value=\"s.tipo\" :selected=\"burger.status == s.tipo\">\r\n                {{ s.tipo }}\r\n              </option>\r\n            </select>\r\n            <button class=\"delete-btn\" @click=\"deleteBurger(burger.id)\">Cancelar</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-else>\r\n      <h2>Não há pedidos registrados</h2>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import Message from \"./Message.vue\";\r\n  export default {\r\n    name: \"Dashboard\",\r\n    data() {\r\n      return {\r\n        burgers: null,\r\n        burger_id: null,\r\n        status: [],\r\n        msg: null,\r\n        data: {\r\n          paes: [], // Coloque aqui os tipos de pães\r\n          carnes: [], // Coloque aqui os tipos de carnes\r\n          opcionais: [] // Coloque aqui os tipos de opcionais\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      async getPedidos() {\r\n        const req = await fetch(\"http://localhost:3000/burgers\");\r\n        const data = await req.json();\r\n        this.burgers = data;\r\n        this.getStatus();\r\n      },\r\n      async getStatus() {\r\n        const req = await fetch(\"http://localhost:3000/status\");\r\n        const data = await req.json();\r\n        this.status = data;\r\n      },\r\n      async deleteBurger(id) {\r\n        const req = await fetch(`http://localhost:3000/burgers/${id}`, {\r\n          method: \"DELETE\"\r\n        });\r\n        const res = await req.json();\r\n        this.msg = `Pedido Removido com Sucesso!`;\r\n        setTimeout(() => {\r\n          this.msg = \"\";\r\n        }, 5000);\r\n        this.getPedidos();\r\n      },\r\n      async updateBurger(event, id) {\r\n        const option = event.target.value;\r\n        const dataJson = JSON.stringify({ status: option });\r\n        const req = await fetch(`http://localhost:3000/burgers/${id}`, {\r\n          method: \"PATCH\",\r\n          headers: { \"Content-Type\": \"application/json\" },\r\n          body: dataJson\r\n        });\r\n        const res = await req.json();\r\n        this.msg = `O Pedido Nº ${res.id} foi atualizado para Status ${res.status}!`;\r\n        setTimeout(() => {\r\n          this.msg = \"\";\r\n        }, 5000);\r\n      },\r\n      async getPaes() {\r\n    const response = await fetch(\"http://localhost:3000/paes\");\r\n    const data = await response.json();\r\n    this.data.paes = data;\r\n  },\r\n  async getCarnes() {\r\n    const response = await fetch(\"http://localhost:3000/carnes\");\r\n    const data = await response.json();\r\n    this.data.carnes = data;\r\n  },\r\n  async getOpcionais() {\r\n    const response = await fetch(\"http://localhost:3000/opcionais\");\r\n    const data = await response.json();\r\n    this.data.opcionais = data;\r\n  },\r\n    },\r\n    async mounted() {\r\n  await this.getPedidos();\r\n  await this.getPaes();\r\n  await this.getCarnes();\r\n  await this.getOpcionais();\r\n},\r\n    components: { Message }\r\n  };\r\n  </script>\r\n\r\n<style scoped>\r\n  #burger-table {\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n  }\r\n\r\n  #burger-table-heading,\r\n  #burger-table-rows,\r\n  .burger-table-row {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n  }\r\n\r\n  #burger-table-heading {\r\n    font-weight: bold;\r\n    padding: 12px;\r\n    border-bottom: 3px solid #333;\r\n  }\r\n\r\n  h2 {\r\n    padding: 50px;\r\n    text-align: center;\r\n  }\r\n\r\n  .burger-table-row {\r\n    width: 100%;\r\n    padding: 12px;\r\n    border-bottom: 1px solid #CCC;\r\n  }\r\n\r\n  #burger-table-heading div,\r\n  .burger-table-row div {\r\n    width: 19%;\r\n  }\r\n\r\n  #burger-table-heading .order-id,\r\n  .burger-table-row .order-number {\r\n    width: 5%;\r\n  }\r\n\r\n  select {\r\n    padding: 12px 6px;\r\n    margin-right: 12px;\r\n  }\r\n\r\n  .delete-btn {\r\n    background-color: #222;\r\n    color:#fcba03;\r\n    font-weight: bold;\r\n    border: 2px solid #222;\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin: 0 auto;\r\n    cursor: pointer;\r\n    transition: .5s;\r\n  }\r\n  \r\n  .delete-btn:hover {\r\n    background-color: transparent;\r\n    color: #222;\r\n  }\r\n  \r\n</style>"],"mappings":"AAgDE,OAAOA,OAAM,MAAO,eAAe;AACnC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,SAAS,EAAE,IAAI;MACfC,MAAM,EAAE,EAAE;MACVC,GAAG,EAAE,IAAI;MACTJ,IAAI,EAAE;QACJK,IAAI,EAAE,EAAE;QAAE;QACVC,MAAM,EAAE,EAAE;QAAE;QACZC,SAAS,EAAE,EAAC,CAAE;MAChB;IACF,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,MAAMC,GAAE,GAAI,MAAMC,KAAK,CAAC,+BAA+B,CAAC;MACxD,MAAMX,IAAG,GAAI,MAAMU,GAAG,CAACE,IAAI,CAAC,CAAC;MAC7B,IAAI,CAACX,OAAM,GAAID,IAAI;MACnB,IAAI,CAACa,SAAS,CAAC,CAAC;IAClB,CAAC;IACD,MAAMA,SAASA,CAAA,EAAG;MAChB,MAAMH,GAAE,GAAI,MAAMC,KAAK,CAAC,8BAA8B,CAAC;MACvD,MAAMX,IAAG,GAAI,MAAMU,GAAG,CAACE,IAAI,CAAC,CAAC;MAC7B,IAAI,CAACT,MAAK,GAAIH,IAAI;IACpB,CAAC;IACD,MAAMc,YAAYA,CAACC,EAAE,EAAE;MACrB,MAAML,GAAE,GAAI,MAAMC,KAAK,CAAE,iCAAgCI,EAAG,EAAC,EAAE;QAC7DC,MAAM,EAAE;MACV,CAAC,CAAC;MACF,MAAMC,GAAE,GAAI,MAAMP,GAAG,CAACE,IAAI,CAAC,CAAC;MAC5B,IAAI,CAACR,GAAE,GAAK,8BAA6B;MACzCc,UAAU,CAAC,MAAM;QACf,IAAI,CAACd,GAAE,GAAI,EAAE;MACf,CAAC,EAAE,IAAI,CAAC;MACR,IAAI,CAACK,UAAU,CAAC,CAAC;IACnB,CAAC;IACD,MAAMU,YAAYA,CAACC,KAAK,EAAEL,EAAE,EAAE;MAC5B,MAAMM,MAAK,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK;MACjC,MAAMC,QAAO,GAAIC,IAAI,CAACC,SAAS,CAAC;QAAEvB,MAAM,EAAEkB;MAAO,CAAC,CAAC;MACnD,MAAMX,GAAE,GAAI,MAAMC,KAAK,CAAE,iCAAgCI,EAAG,EAAC,EAAE;QAC7DC,MAAM,EAAE,OAAO;QACfW,OAAO,EAAE;UAAE,cAAc,EAAE;QAAmB,CAAC;QAC/CC,IAAI,EAAEJ;MACR,CAAC,CAAC;MACF,MAAMP,GAAE,GAAI,MAAMP,GAAG,CAACE,IAAI,CAAC,CAAC;MAC5B,IAAI,CAACR,GAAE,GAAK,eAAca,GAAG,CAACF,EAAE,+BAA+BE,GAAG,CAACd,MAAO,GAAE;MAC5Ee,UAAU,CAAC,MAAM;QACf,IAAI,CAACd